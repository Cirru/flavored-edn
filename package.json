{
  "name": "favored-edn",
  "version": "0.1.0",
  "description": "Customized EDN formatter",
  "main": "index.js",
  "scripts": {
    "deploy": "clj -A:release",
    "m2": "clj -A:release install",
    "watch": "shadow-cljs watch client",
    "serve": "http-server dist -s",
    "build-cdn": "shadow-cljs clj-run build.main/build-cdn",
    "build": "shadow-cljs clj-run build.main/build",
    "up": "yarn build-cdn && tiye-up",
    "page": "shadow-cljs clj-run build.main/page",
    "test": "shadow-cljs compile test && node target/test.js",
    "ln": "cd target && rm -f entry && ln -s ../entry"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Cirru/favored-edn.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^0.12.1",
    "shadow-cljs": "^2.8.92"
  },
  "dependencies": {
    "shortid": "^2.2.15"
  }
}
